	harfHarf[9]=["göstermek","kullanmak","özellikle","uygulamak","hissetmek","araştırma","kaldırmak","belirtmek","kaybetmek","gerçekten","teknoloji","sürdürmek","göndermek","açıklamak","kurtulmak","farketmek","sağlanmak","söylenmek","yaklaşmak","etkilemek","seyretmek","davranmak","incelemek","doldurmak","kurtarmak","yükselmek","enflasyon","hastahane","yakalamak","toplumsal","toplanmak","demokrasi","üretilmek","alışveriş","bağlanmak","neredeyse","paylaşmak","yaptırmak","ilerlemek"];

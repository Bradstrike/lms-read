	var egzersizBasladi = false;
	
	$(document).ready(function(){
		$(".goster-buton").click(function(){
			kenarlik()
		});
        $(document).keypress(function (e){
			var asciikodu;
			if(window.event) {asciikodu = e.keyCode}
			else if(e.which) {asciikodu = e.which}
			if(asciikodu == 13 && egzersizBasladi && durum == 0){
				kenarlik();
			}else if(asciikodu == 13 && egzersizBasladi && durum == 1){
				kontrolEt();
			}
		});
		$("#sure-ayarla").click(function(){
			$("#sure-ayarla-menu").show();
		});
	});
	
	
	
	var aktifKelime = "";
	function kenarlik(){
		$(".goster-buton").hide();
		$(".metincikler").text("");
		$(".metincikler").css('background','#e7e7e7');
		durum = 2;
		var karakterSayisi = seviye;
		var rastgeleKelime;
		if(karakterSayisi <= 5){
			rastgeleKelime = X(0,harfHarf[karakterSayisi].length-1);
			aktifKelime = harfHarf[karakterSayisi][rastgeleKelime];		
		}else if(karakterSayisi >= 6 && karakterSayisi <= 11){
			var kelimeTuru = X(0,1);
			if(kelimeTuru == 0){
				rastgeleKelime = X(0,harfHarf[karakterSayisi].length-1);
				aktifKelime = harfHarf[karakterSayisi][rastgeleKelime];
			}else if(kelimeTuru == 1){
				var birinciKelime = X(2,karakterSayisi-2);
				var ikinciKelime = karakterSayisi - birinciKelime;
				var kelimeBir = X(0,harfHarf[birinciKelime].length-1);
				var kelimeIki = X(0,harfHarf[ikinciKelime].length-1);
				aktifKelime = harfHarf[birinciKelime][kelimeBir] + " " + harfHarf[ikinciKelime][kelimeIki];
			}
		}else if(karakterSayisi >= 12 && karakterSayisi <= 16){
			var birinciKelime = X(5,karakterSayisi-5);
			var ikinciKelime = karakterSayisi - birinciKelime;
			var kelimeBir = X(0,harfHarf[birinciKelime].length-1);
			var kelimeIki = X(0,harfHarf[ikinciKelime].length-1);
			aktifKelime = harfHarf[birinciKelime][kelimeBir] + " " + harfHarf[ikinciKelime][kelimeIki];
		}else if(karakterSayisi >= 17 && karakterSayisi <= 28){
			var ortalama = Math.floor(karakterSayisi / 3);
			var karakterSayilari = [];
			var birinciKelime = ortalama;
			var eksiBirmi = X(0,1);
			var ikinciKelime;
			if(eksiBirmi == 0) ikinciKelime = ortalama - 1;
			else ikinciKelime = ortalama + 1;
			var ucuncuKelime = karakterSayisi - birinciKelime - ikinciKelime;
			karakterSayilari.push(birinciKelime);
			karakterSayilari.push(ikinciKelime);
			karakterSayilari.push(ucuncuKelime);
			var karistirilmisKelimeler = karistir(karakterSayilari);
			
			var kelimeBir = X(0,harfHarf[karistirilmisKelimeler[0]].length-1);
			var kelimeIki = X(0,harfHarf[karistirilmisKelimeler[1]].length-1);
			var kelimeUc = X(0,harfHarf[karistirilmisKelimeler[2]].length-1);
			aktifKelime = harfHarf[karistirilmisKelimeler[0]][kelimeBir] + " " + harfHarf[karistirilmisKelimeler[1]][kelimeIki] + " " + harfHarf[karistirilmisKelimeler[2]][kelimeUc];
		}else if(karakterSayisi >= 29){
			var ortalama = Math.floor(karakterSayisi / 4);
			var karakterSayilari = [];
			var birinciKelime = ortalama;
			var eksiBirmi = X(0,1);
			var ikinciKelime;
			if(eksiBirmi == 0) ikinciKelime = ortalama - 1;
			else ikinciKelime = ortalama + 1;
			var ucuncuKelime = ortalama;
			var dorduncuKelime = karakterSayisi - birinciKelime - ikinciKelime - ucuncuKelime;
			karakterSayilari.push(birinciKelime);
			karakterSayilari.push(ikinciKelime);
			karakterSayilari.push(ucuncuKelime);
			karakterSayilari.push(dorduncuKelime);
			var karistirilmisKelimeler = karistir(karakterSayilari);
			var kelimeBir = X(0,harfHarf[karistirilmisKelimeler[0]].length-1);
			var kelimeIki = X(0,harfHarf[karistirilmisKelimeler[1]].length-1);
			var kelimeUc = X(0,harfHarf[karistirilmisKelimeler[2]].length-1);
			var kelimeDort = X(0,harfHarf[karistirilmisKelimeler[3]].length-1);
			aktifKelime = harfHarf[karistirilmisKelimeler[0]][kelimeBir] + " " + harfHarf[karistirilmisKelimeler[1]][kelimeIki] + " " + harfHarf[karistirilmisKelimeler[2]][kelimeUc] + " " +harfHarf[karistirilmisKelimeler[3]][kelimeDort];
		}
		setTimeout(function(){$(".metincikler").css('border-color','#598BB7')},100);
		setTimeout(function(){$(".metincikler").css('border-color','white')},200);
		setTimeout(function(){$(".metincikler").css('border-color','#598BB7')},300);
		setTimeout(function(){$(".metincikler").css('border-color','white')},400);
		setTimeout(function(){$(".metincikler").css('border-color','#598BB7')},500);
		setTimeout(function(){
			durum = 1;
			$(".metincikler").css('border-color','white');
			setTimeout(function(){$(".metincikler").text(aktifKelime)},50);
			setTimeout(function(){$(".metincikler").text("")},50 + seviyeler[hiz]);
			setTimeout(function(){cevapPenceresiGoster()},150 + seviyeler[hiz]);
		},600);	
	}
	
	function cevapPenceresiGoster(){
		$(".cevap-kutusu").show();
		$("#cevap").val("").focus();
	}
	
	var toplamDogru = 0;
	var seviyeAtlamaDogru = 0;
	var seviye = 3;
	function kontrolEt(){
		durum = 0;
		var cevap = $("#cevap").val().turkishToLower();
		if(aktifKelime == cevap){
			$(".metincikler").css('border-color','#59B7A5');
			$(".metincikler").css('background','#e7e7e7 url("checked2.svg") center center no-repeat');
			$(".metincikler").css('background-size','10%');
			toplamDogru++;
			seviyeAtlamaDogru++;
			if(seviyeAtlamaDogru == 6){
				seviyeAtlamaDogru = 0;
				seviye++;
			}
		}else{
			$(".metincikler").css('border-color','#C5606B');
			$(".metincikler").css('background','#e7e7e7 url("unchecked.svg") center center no-repeat');
			$(".metincikler").css('background-size','9%');
		}
		$(".calis-bilgiler").text("Karakter: " + seviye + ", Sonraki Seviye: " + (6 - seviyeAtlamaDogru));
		$(".cevap-kutusu").hide();
		$(".goster-buton").show();
	}
	
	var egzersizBasladi = false;
	function testiBaslat(){
		egzersizBasladi = true;
		$(".tutucu").css('opacity','0.5');
		$(".testi-baslat-tutucu, #karanlik").hide();
	}
	
	function harfAyarla(x){
		$(".tutucu .menu").hide();
		$(".harf-basligi").text(x+" Harf");
		seviye = x;
		$(".calis-bilgiler").text("Karakter: " + seviye + ", Sonraki Seviye: " + (6 - seviyeAtlamaDogru));
		
	}
	
	function ozelHarfAktar(){
		$("#sure-ayarla-menu").hide();
		$(".tutucu .menu").hide();
		var ozelHarf = parseInt($("#ozel-harf").val());
		harfAyarla(ozelHarf);
	}
	
	function seviyeAyarla(x){
		$(".tutucu .menu").hide();
		hiz = x;
		$(".seviye-basligi").text(x+". Seviye ~ "+seviyeler[hiz]+ "ms");
	}
	
	// İki Değişken Arasından Rastgele = X
	function X(x,y){return Math.floor(Math.random()*(y-x+1)+x);}
	
	function karistir(v){for(var j,x,i=v.length;i;j=parseInt(Math.random()*i),x=v[--i],v[i]=v[j],v[j]=x);return v;}
	</script>
	
